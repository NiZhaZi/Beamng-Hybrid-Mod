{
    "Hybrid_Configure_01": {
		"information":{
			"name":"Hybrid Configure 01",
			"value": 0
		},
        "slots": [
			["type", "default", "description"],
			["Shift_Logic", "", "Shift Logic"],
			["Single_Motors", "", "Single Motors"],
		],
		"slotType" : "Hybrid_Configure",
		"variables": [
			["name", "type", "unit", "category", "default", "min", "max", "title", "description"],
		],

		"singleMotors":{
			"motorNames": ["rearMotor", "frontMotor"],
			"motorType": "partTime",
			"compareNodes1": "frontDriveShaft",
			"compareNodes2": "rearDriveShaft",
			"maxRPMdiff": 50,
		},
        
		"hybridControl":{
			//get all motors Names
			"motorNames": ["frontDriveShaft", "rearDriveShaft"],
			//EV ondemand AWD settings
			"mainMotorNames": ["rearDriveShaft"],
			"subMotorNames": ["frontDriveShaft"],
			"ondemandMaxRPM" : 50,
			//mode settings
			"enableModes" : ["hybrid", "fuel", "electric", "auto", "reev"],
			"defaultMode" : "hybrid",
			//ratio1 for hybrid mode, ratio2 for EV mode
			"motorRatio1": 1.05,
			"motorRatio2": 2.50,
			//comfortable regen settings
			"ifComfortRegen": true,
			"comfortRegenBegine": 0.75,//should be bigger than comfortRegenEnd
			"comfortRegenEnd": 0.15,
			//auto mode settings
			"startVelocity" : 36,
			"connectVelocity" : 41,
			"lowSpeed": 0.5, // km/h
		},

		"motorControl":{
			"motorNames": ["frontDriveShaft", "rearDriveShaft", "gearbox", "rearMotor", "frontMotor"],
		},

		"powerGenerator":{
			"powerGeneratorName": "gearbox",
			"SOC": 80,
			"defaultMode": "auto",
		}

		"gearbox":{
			"torque":[
				["rpm", "torque"],
				[0, 120.00],
				[10000, 120.00],
			],
			"electricsThrottleName": "powerGenerator",
			"maximumWantedRegenTorque": "$=350*(1-(0.5 or 0))*(1 or 1)",

		},

	},

	"Single_Motors": {
		"information":{
			"name":"Single Motors Control",
			"value": 0
		},
        "slots": [
			["type", "default", "description"],
		],
		"slotType" : "Single_Motors",

		"controller": [
			["fileName"],
			["hybrid/singleMotors", {"name": "singleMotors"}],
		],
	},

	"Automatic_Gearbox": {
		"information":{
			"name":"Shift Logic Automatic Gearbox",
			"value": 0
		},
        "slots": [
			["type", "default", "description"],
		],
		"slotType" : "Shift_Logic",

		"vehicleController":{
			"shiftLogicName": "automaticGearbox",
		},
	},

	"DCT_Gearbox": {
		"information":{
			"name":"Shift Logic DCT Gearbox",
			"value": 0
		},
        "slots": [
			["type", "default", "description"],
		],
		"slotType" : "Shift_Logic",

		"vehicleController":{
			"shiftLogicName": "dctGearbox",
		},
	},

	"CVT_Gearbox": {
		"information":{
			"name":"Shift Logic CVT Gearbox",
			"value": 0
		},
        "slots": [
			["type", "default", "description"],
		],
		"slotType" : "Shift_Logic",

		"vehicleController":{
			"shiftLogicName": "cvtGearbox",
		},
	},

}